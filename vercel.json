{
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/icon/(.*)",
      "dest": "/icon/$1"
    },
    {
      "src": "/api/v1/ai/(.*)",
      "dest": "http://47.98.234.55:8000/api/v1/ai/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ]
}
